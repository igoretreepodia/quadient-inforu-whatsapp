{
  "name": "whatsapp-inforu-gcf-node",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "functions-framework --target=inforu-whatsapp",
    "deploy": "gcloud functions deploy inforu-whatsapp --gen2 --runtime=nodejs18 --entry-point=inforu-whatsapp --trigger-http --allow-unauthenticated --set-env-vars=INFORU_USERNAME=$INFORU_USERNAME,INFORU_TOKEN=$INFORU_TOKEN,INFORU_SEGMENTS_PER_SECOND=$INFORU_SEGMENTS_PER_SECOND,CREDENTIALS=$CREDENTIALS"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.1.1",
    "node-cache": "^5.1.2",
    "axios": "^1.4.0"
  }
}
